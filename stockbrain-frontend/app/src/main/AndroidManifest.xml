<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_MEDIA_IMAGES" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Stockbrain"
        android:usesCleartextTraffic="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:requestLegacyExternalStorage="true">
        <activity
            android:name=".logica.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".logica.RegistroUsuarios"
            android:exported="true"
            android:parentActivityName=".logica.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".logica.MainActivity" />
        </activity>
        <activity android:name=".logica.InicioSesion"
            android:exported="true"
            android:parentActivityName=".logica.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".logica.MainActivity" />
        </activity>

        <activity android:name=".logica.home.Home"
            android:exported="true"
            android:parentActivityName=".logica.MainActivity">
        </activity>

        <activity android:name=".logica.CrearTienda"
            android:exported="true"
            android:parentActivityName=".logica.MainActivity">
        </activity>

        <activity android:name=".logica.ListaTiendasUsuarios"
            android:exported="true"
            android:parentActivityName=".logica.MainActivity">
        </activity>

        <activity android:name=".logica.home.HomeCrearProductos"
            android:networkSecurityConfig="@xml/network_security_config"
            android:exported="true"
            android:parentActivityName=".logica.MainActivity">
        </activity>

        <activity android:name=".logica.Ajustes"
            android:exported="true"
            android:parentActivityName=".logica.MainActivity">
        </activity>

        <activity android:name=".logica.home.HomeListaProductos"
            android:exported="true"
            android:parentActivityName=".logica.MainActivity">
        </activity>

        <activity android:name=".logica.Faq"
            android:exported="true"
            android:parentActivityName=".logica.MainActivity">
        </activity>

        <activity android:name=".logica.home.HomeEditarProducto"
            android:exported="true"
            android:parentActivityName=".logica.MainActivity">
        </activity>

        <activity android:name=".logica.EditarPerfil"
            android:exported="true"
            android:parentActivityName=".logica.MainActivity">
        </activity>

        <activity android:name=".logica.home.HomeVentas"
            android:exported="true"
            android:parentActivityName=".logica.MainActivity">
        </activity>

        <activity android:name=".logica.home.HomeProductosVender"
            android:exported="true"
            android:parentActivityName=".logica.MainActivity">
        </activity>

        <activity android:name=".logica.home.HomeAlertas"
            android:exported="true"
            android:parentActivityName=".logica.MainActivity">
        </activity>

        <activity android:name=".logica.home.HomeDashboard"
            android:exported="true"
            android:parentActivityName=".logica.MainActivity">
        </activity>

        <activity android:name=".logica.ListaProductosUsuario"
            android:exported="true"
            android:parentActivityName=".logica.MainActivity">
        </activity>

    </application>
</manifest>